.fr
  float right
.pa
  position absolute
.pf
  position fixed
.inline
  display inline-block
  vertical-align middle

.text-flow
.mult-text-flow
  display -webkit-box
  -webkit-box-orient vertical
  -webkit-line-clamp 3
  overflow hidden

.logo-icon
  background url("../img/logo-w.png") no-repeat
  background-size contain

.projects-item
  .card
    cursor pointer
    &:hover
      box-shadow 0 8px 17px 2px rgba(0, 0, 0, 0.14), 0 3px 14px 2px rgba(0, 0, 0, 0.12), 0 5px 5px -3px rgba(0, 0, 0, 0.2)
  .card-title
    padding 10px 0 0 20px
  .card-content
    padding-top 10px


  .star-tip
    color #666
    padding-top 10px
    font-size 14px
    margin-right 20px

.brand-logo
  padding-left 20px !important
  .logo-icon
    width 28px
    height 31px
    margin-right 0px

